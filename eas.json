{
  "cli": {
    "version": ">= 0.55.1"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
		"distribution": "internal",
		"env": {
			"GOOGLE_SERVICES_FILE": "./android/app/google-services.json"
		}
    },
    "production": {
		"ios": {
			"autoIncrement": true
		},
		"android": {
			"autoIncrement": true
		},
		"env": {
			"GOOGLE_SERVICES_FILE": "./android/app/google-services.json"
		}
	}
  },
  "submit": {
    "production": {
		
	}
  }
}
